{
    "name": "rejourney-backend",
    "version": "1.1.0",
    "description": "Rejourney Session Recording Backend API",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint src",
        "test": "vitest run",
        "test:watch": "vitest",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:studio": "drizzle-kit studio",
        "db:seed": "tsx src/db/seed.ts",
        "db:seed:selfhosted": "tsx src/db/seedSelfHosted.ts",
        "usage:backfill:device": "node --import tsx/esm scripts/backfill_device_usage.ts",
        "usage:backfill:device:dry-run": "node --import tsx/esm scripts/backfill_device_usage.ts --dry-run",
        "worker:ingest": "tsx src/worker/ingestWorker.ts",
        "worker:billing": "tsx src/worker/billingWorker.ts",
        "worker:retention": "tsx src/worker/retentionWorker.ts",
        "worker:alerts": "tsx src/worker/alertWorker.ts"
    },
    "author": "Rejourney",
    "license": "SSPL-1.0",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.985.0",
        "@aws-sdk/s3-request-presigner": "^3.679.0",
        "better-auth": "^1.0.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "drizzle-kit": "^1.0.0-beta.12",
        "drizzle-orm": "^1.0.0-beta.12",
        "express": "^4.21.1",
        "express-rate-limit": "^7.4.1",
        "geoip-lite": "^1.4.10",
        "helmet": "^8.0.0",
        "ioredis": "^5.4.1",
        "nodemailer": "^8.0.1",
        "npm": "^11.7.0",
        "pg": "^8.13.1",
        "pino": "^9.5.0",
        "pino-http": "^10.3.0",
        "pino-pretty": "^13.0.0",
        "stripe": "^20.1.0",
        "tsx": "^4.19.2",
        "ua-parser-js": "^2.0.7",
        "uuid": "^11.0.3",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^4.17.25",
        "@types/geoip-lite": "^1.4.4",
        "@types/node": "^22.9.0",
        "@types/nodemailer": "^6.4.16",
        "@types/pg": "^8.11.10",
        "@types/ua-parser-js": "^0.7.39",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.50.1",
        "@typescript-eslint/parser": "^8.50.1",
        "eslint": "^9.14.0",
        "typescript": "^5.4.5",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=24.0.0"
    }
}